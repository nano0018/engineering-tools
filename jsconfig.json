{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@assets/*": [
                "src/assets/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@/*": [
                "src/*"
            ],
            "@routes/*": [
                "src/routes/*"
            ],
            "@HomePage/*": [
                "src/pages/HomePage/*"
            ],
            "@AboutPage/*": [
                "src/pages/AboutPage/*"
            ],
            "@NotFoundPage/*": [
                "src/pages/NotFoundPage/*"
            ]
        }
    }
}