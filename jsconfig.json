{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@assets/*": [
                "src/assets/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@/*": [
                "src/*"
            ],
            "@routes/*": [
                "src/routes/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@data/*": [
                "src/data/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@NotFoundCard/*": [
                "src/components/NotFoundCard/*"
            ],
            "@MessageCard/*": [
                "src/components/MessageCard/*"
            ],
            "@UnderConstruction/*": [
                "src/pages/UnderConstruction/*"
            ],
            "@Footer/*": [
                "src/components/Footer/*"
            ],
            "@MainLayout/*": [
                "src/components/MainLayout/*"
            ]
        }
    }
}