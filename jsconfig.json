{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@assets/*": [
                "src/assets/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@/*": [
                "src/*"
            ],
            "@routes/*": [
                "src/routes/*"
            ],
            "@Navbar/*": [
                "src/components/Navbar/*"
            ],
            "@Layout/*": [
                "src/components/Layout/*"
            ],
            "@calculations/*": [
                "src/utils/calculations/*"
            ],
            "@units/*": [
                "src/utils/calculations/units/*"
            ]
        }
    }
}
